    "start": "babel . --watch --out-dir compiled --presets=es2015,react --ignore=node_modules,compiled --source-maps inline",
    "resetdb": "psql -f server/db/setuphotspotdb.sql",
    "serverdb": "npm run server & npm run startdb"
